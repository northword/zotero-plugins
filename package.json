  "version": "1.0.0",
  "description": "Zotero 插件合集",
  "main": "index.js",
  "scripts": {
    "get-info": "tsx src/backend/index.ts fetchPlugins",
    "get-chart": "tsx src/backend/index.ts charts",
    "write-md": "tsx src/backend/index.ts md",
    "debug:info": "cross-env NODE_ENV=development npm run get-info",
    "debug:chart": "cross-env NODE_ENV=development npm run get-chart",
    "website": "docsify serve docs",
    "website:dev": "vite",
    "website:build": "run-p type-check \"build-only {@}\" --",
    "website:preview": "vite preview",
    "website:build-only": "vite build",
    "website:type-check": "vue-tsc --build --force",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/northword/zotero-plugins.git"
