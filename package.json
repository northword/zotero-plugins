  "devDependencies": {
    "@highcharts/dashboards": "^1.3.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/adm-zip": "^0.5.5",
    "@types/node": "^20.11.17",
    "@types/xml2js": "^0.4.14",
    "@vitalets/google-translate-api": "^9.2.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/cli": "^5.0.8",
    "@vue/tsconfig": "^0.5.1",
    "adm-zip": "^0.5.10",
