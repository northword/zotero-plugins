    "highcharts": "^11.3.0",
    "jsonc": "^2.0.0",
    "markdown-table-ts": "^1.0.3",
    "npm-run-all2": "^6.1.1",
    "octokit": "^3.1.2",
    "tsx": "^4.7.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vue-tsc": "^1.8.27",
